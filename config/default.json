{
  "logger": {
    "level": "all"
  },
  "server": {
    "port": 8080
  },
  "crypto": {
    "hash": {
      "length": 128
    }
  },
  "restLogLevel": "dev",
  "lusca": {
    "csrf": false,
    "xssProtection": true,
    "nosniff": true,
    "hsts": {
      "maxAge": 31536000,
      "includeSubDomains": true,
      "preload": true
    }
  },
  "session": {
    "ex": 604800
  },
  "rabbit": {
    "url": "amqp://localhost"
  },
  "redis": {
    "URL": "redis://127.0.0.1:6379",
    "EX": 60
  }
}
